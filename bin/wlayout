#!/usr/bin/env ruby
# -*- encoding: utf-8 -*-

$LOAD_PATH.unshift File.dirname(__FILE__) + '/../lib'
$WLAYOUT_PATH = File.expand_path(File.dirname(__FILE__)) + '/..'

gem 'ruby-wmctrl', '0.0.5'
require 'wmctrl'
require 'wlayout'


workspace = WLayout::Workspace.new
windows = WLayout::Windows.new workspace

